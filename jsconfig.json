{
	"extends": "./.svelte-kit/tsconfig.json",
	"paths": {
		"$lib": "./src/lib/"
	},
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
}
